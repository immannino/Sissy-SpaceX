
{
    "manifest_version": 3,
    "name": "Sissy SpaceX",
    "description": "A helpful tool for ensuring everything is right in the world.",
    "version": "1.0",
    "action": {
      "default_icon": "assets/logo.png"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://x.com/*"
    ]
}